#include<stdio.h>
#include<conio.h>
int main()
{
int A,B,C;
printf("Enter number A:");
scanf("%d",&A);
printf("Enter number B:");
scanf("%d",&B);
printf("Enter number C:");
scanf("%d",&C);
if (A>B && B>C)
printf("\nA is greatest number");
else if (B>A && B>C)
printf("\nB is greatest number");
else 
printf("\nC is greatest number");
getch();
}
